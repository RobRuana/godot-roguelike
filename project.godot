; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "TileMap",
"class": "Board",
"language": "GDScript",
"path": "res://src/Board.gd"
}, {
"base": "KinematicBody2D",
"class": "Entity",
"language": "GDScript",
"path": "res://src/entities/Entity.gd"
}, {
"base": "Node2D",
"class": "FOV",
"language": "GDScript",
"path": "res://src/FOV.gd"
}, {
"base": "Node",
"class": "PlayerInput",
"language": "GDScript",
"path": "res://src/PlayerInput.gd"
}, {
"base": "Node2D",
"class": "Scene",
"language": "GDScript",
"path": "res://src/Scene.gd"
}, {
"base": "Node2D",
"class": "ShadowMapper",
"language": "GDScript",
"path": "res://src/ShadowMapper.gd"
}, {
"base": "Node2D",
"class": "Tile",
"language": "GDScript",
"path": "res://src/Tile.gd"
}, {
"base": "TileMap",
"class": "TileVisibilityPainter",
"language": "GDScript",
"path": "res://src/TileVisibilityPainter.gd"
}, {
"base": "Node2D",
"class": "globals",
"language": "GDScript",
"path": "res://src/globals.gd"
} ]
_global_script_class_icons={
"Board": "",
"Entity": "",
"FOV": "",
"PlayerInput": "",
"Scene": "",
"ShadowMapper": "",
"Tile": "",
"TileVisibilityPainter": "",
"globals": ""
}

[application]

config/name="crawl"
run/main_scene="res://src/Scene.tscn"
config/icon="res://icon.png"

[autoload]

globals="*res://src/globals.gd"
events="*res://src/events.gd"

[display]

window/size/width=1
window/size/height=1
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "gut" )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[rendering]

quality/2d/use_pixel_snap=true
environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
